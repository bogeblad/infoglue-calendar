<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd" version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
	
	<portlet>
		<description>Webwork</description>
		<portlet-name>WebworkDispatcherPortlet</portlet-name>
		<display-name>WebworkDispatcherPortlet</display-name>
		<portlet-class>com.opensymphony.webwork.portlet.dispatcher.PortletDispatcher</portlet-class>
		
		<init-param>
			<name>view</name>
			<value>ViewCalendarAdministration.action</value>
		</init-param>

		<expiration-cache>-1</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-info>
			<title>WebWork Dispatcher portlet</title>
			<short-title>WebworkDispatcherPortlet</short-title>
			<keywords>WebworkDispatcherPortlet</keywords>
		</portlet-info>
	</portlet>

</portlet-app>
