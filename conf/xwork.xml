<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">
<xwork>
	<!-- Include webwork defaults (from WebWork JAR). -->
	<include file="webwork-default.xml"/>
	
	<!-- Configuration for the default package. -->
	<package name="default" extends="webwork-default">
		<default-interceptor-ref name="fileUploadStack"/>
		<!-- HERE YOU PUT ACTIONS FOR THE NORMAL WEBAPP -->
	</package>
	
	<package name="mypackage" namespace="/WebworkDispatcherPortlet" extends="webwork-default">
		    		
		<default-interceptor-ref name="defaultStack"/>
		
		<action name="ViewCalendarAdministration" class="org.infoglue.calendar.actions.ViewCalendarAdministrationAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewCalendarAdministration.jsp</param>
			</result>
		</action>
		
		<!-- CALENDAR ACTIONS -->
		<action name="ViewCalendarList" class="org.infoglue.calendar.actions.ViewCalendarListAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewCalendarList.jsp</param>
			</result>
		</action>
		<action name="CreateCalendar" class="org.infoglue.calendar.actions.CreateCalendarAction">
			<result name="input" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/createCalendar.jsp</param>
            </result>
            <result name="error" type="eventProcessed">
				<param name="viewAction">CreateCalendar!input.action</param>
			</result>
            <result name="success" type="eventProcessed">
				<param name="viewAction">ViewCalendarList.action</param>
			</result>
		</action>
		<action name="ViewCalendar" class="org.infoglue.calendar.actions.ViewCalendarAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewCalendar.jsp</param>
			</result>
			<result name="successPublic" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewCalendarPublic.jsp</param>
			</result>
		</action>
		<action name="UpdateCalendar" class="org.infoglue.calendar.actions.UpdateCalendarAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCalendarList.action</param>
			</result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">ViewCalendar.action</param>
			</result>
		</action>
		<action name="DeleteCalendar" class="org.infoglue.calendar.actions.DeleteCalendarAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCalendarList.action</param>
			</result>
		</action>
		
		<!-- LOCATION ACTIONS -->
		<action name="ViewLocationList" class="org.infoglue.calendar.actions.ViewLocationListAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewLocationList.jsp</param>
			</result>
		</action>
		<action name="CreateLocation" class="org.infoglue.calendar.actions.CreateLocationAction">
			<result name="input" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/createLocation.jsp</param>
            </result>
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewLocationList.action</param>
			</result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">CreateLocation!input.action</param>
			</result>
		</action>
		<action name="ViewLocation" class="org.infoglue.calendar.actions.ViewLocationAction">
			<result name="success" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/viewLocation.jsp</param>
            </result>
		</action>
		<action name="UpdateLocation" class="org.infoglue.calendar.actions.UpdateLocationAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewLocationList.action</param>
			</result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">ViewLocation.action</param>
			</result>
		</action>
		<action name="DeleteLocation" class="org.infoglue.calendar.actions.DeleteLocationAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewLocationList.action</param>
			</result>
		</action>
		
		<!-- CATEGORY ACTIONS -->
		<action name="ViewCategoryList" class="org.infoglue.calendar.actions.ViewCategoryListAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewCategoryList.jsp</param>
			</result>
		</action>
		<action name="CreateCategory" class="org.infoglue.calendar.actions.CreateCategoryAction">
			<result name="input" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/createCategory.jsp</param>
            </result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">CreateCategory!input.action</param>
			</result>
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCategoryList.action</param>
			</result>
		</action>
		<action name="ViewCategory" class="org.infoglue.calendar.actions.ViewCategoryAction">
			<result name="success" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/viewCategory.jsp</param>
            </result>
		</action>
		<action name="UpdateCategory" class="org.infoglue.calendar.actions.UpdateCategoryAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCategoryList.action</param>
			</result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">ViewCategory.action</param>
			</result>
		</action>
		<action name="DeleteCategory" class="org.infoglue.calendar.actions.DeleteCategoryAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCategoryList.action</param>
			</result>
		</action>
		
		<!-- EVENT ACTIONS -->
		<action name="CreateEvent" class="org.infoglue.calendar.actions.CreateEventAction">
			<result name="input" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/createEvent.jsp</param>
            </result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">CreateEvent!input.action</param>
			</result>
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewEvent.action?eventId=${newEvent.id}</param>
				<param name="parse">true</param>
			</result>
			<!--
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCalendar.action?calendarId=${calendarId}&amp;mode=${mode}&amp;startDateTime=${startDateTime}&amp;endDateTime=${endDateTime}</param>
				<param name="parse">true</param>
			</result>
			-->
		</action>
		<action name="ViewEvent" class="org.infoglue.calendar.actions.ViewEventAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewEvent.jsp</param>
			</result>
			<result name="successPublic" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewEventPublic.jsp</param>
			</result>
			<result name="successPublicGU" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/gu/viewEventPublic.jsp</param>
			</result>
		</action>
		<action name="UpdateEvent" class="org.infoglue.calendar.actions.UpdateEventAction">
			<result name="error" type="eventProcessed">
				<param name="viewAction">ViewEvent.action?eventId=${eventId}&amp;calendarId=${calendarId}&amp;mode=${mode}</param>
				<param name="parse">true</param>
			</result>
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCalendar.action?calendarId=${calendarId}&amp;mode=${mode}&amp;startDateTime=${startDateTime}&amp;endDateTime=${endDateTime}</param>
				<param name="parse">true</param>
			</result>
		</action>
		<action name="DeleteEvent" class="org.infoglue.calendar.actions.DeleteEventAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewCalendar.action?calendarId=${calendarId}&amp;mode=${mode}&amp;startDateTime=${startDateTime}&amp;endDateTime=${endDateTime}</param>
				<param name="parse">true</param>
			</result>
		</action>
		
		<action name="ViewEventList" class="org.infoglue.calendar.actions.ViewEventListAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewEventList.jsp</param>
			</result>
			<result name="successShort" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewEventShortList.jsp</param>
			</result>
			<result name="successGU" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/gu/viewEventList.jsp</param>
			</result>
			<result name="successShortGU" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/gu/viewEventShortList.jsp</param>
			</result>
		</action>
		
		
		
		
		<!-- RESOURCE ACTIONS -->
		<action name="CreateResource" class="org.infoglue.calendar.actions.CreateResourceAction">
			<result name="error" type="eventProcessed">
				<param name="viewAction">ViewEvent.action?eventId=${eventId}&amp;calendarId=${calendarId}&amp;mode=${mode}</param>
				<param name="parse">true</param>
			</result>
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewEvent.action?eventId=${eventId}&amp;calendarId=${calendarId}&amp;mode=${mode}</param>
				<param name="parse">true</param>
			</result>
		</action>
		<action name="DeleteResource" class="org.infoglue.calendar.actions.DeleteResourceAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewEvent.action?eventId=${eventId}&amp;calendarId=${calendarId}&amp;mode=${mode}</param>
				<param name="parse">true</param>
			</result>
		</action>
		
		
		<!-- ENTRY ACTIONS -->
		<action name="CreateEntry" class="org.infoglue.calendar.actions.CreateEntryAction">
			<result name="input" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/createEntry.jsp</param>
            </result>
			<result name="inputPublic" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/createEntryPublic.jsp</param>
            </result>
			<result name="inputPublicGU" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/gu/createEntryPublic.jsp</param>
            </result>
            <result name="success" type="eventProcessed">
				<param name="viewAction">ViewEvent.action?eventId=${eventId}</param>
				<param name="parse">true</param>
			</result>
            <result name="successPublic" type="eventProcessed">
				<param name="viewAction">ViewEvent!public.action?eventId=${eventId}</param>
				<param name="parse">true</param>
			</result>
            <result name="maximumReached" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/maximumEntriesReached.jsp</param>
			</result>
            <result name="maximumReachedPublic" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/maximumEntriesReachedPublic.jsp</param>
			</result>
            <result name="maximumReachedPublicGU" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/gu/maximumEntriesReachedPublic.jsp</param>
			</result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">CreateEntry!input.action</param>
			</result>
			<result name="errorPublic" type="eventProcessed">
				<param name="viewAction">CreateEntry!inputPublic.action</param>
			</result>
			<result name="errorPublicGU" type="eventProcessed">
				<param name="viewAction">CreateEntry!inputPublicGU.action</param>
			</result>
			
		</action>
		<action name="ViewEntrySearch" class="org.infoglue.calendar.actions.ViewEntrySearchAction">
			<result name="input" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/viewEntrySearch.jsp</param>
            </result>
            <result name="success" type="portletDispatch">
            	<param name="dispatchTo">/jsp/portlet/viewEntrySearch.jsp</param>
            </result>
		</action>
		<action name="ViewEntry" class="org.infoglue.calendar.actions.ViewEntryAction">
			<result name="success" type="portletDispatch">
				<param name="dispatchTo">/jsp/portlet/viewEntry.jsp</param>
			</result>
		</action>
		<action name="UpdateEntry" class="org.infoglue.calendar.actions.UpdateEntryAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewEntrySearch.action</param>
				<param name="parse">true</param>
			</result>
			<result name="error" type="eventProcessed">
				<param name="viewAction">ViewEntry.action</param>
			</result>
		</action>
		<action name="DeleteEntry" class="org.infoglue.calendar.actions.DeleteEntryAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewEntrySearch.action</param>
				<param name="parse">true</param>
			</result>
		</action>
		<action name="EmailEntries" class="org.infoglue.calendar.actions.EmailEntriesAction">
			<result name="success" type="eventProcessed">
				<param name="viewAction">ViewEntrySearch.action</param>
				<param name="parse">true</param>
			</result>
		</action>
		
	</package>
</xwork>
